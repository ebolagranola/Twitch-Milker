{
  "name":"Twitch Milker",
  "description":"",
  "version":"1",
  "manifest_version":3,
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_title": "Twitch Milker",
    "default_popup": "./popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  }
}
